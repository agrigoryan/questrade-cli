{
  "name": "@agrigoryan/questrade-cli",
  "version": "0.1.1",
  "description": "CLI for the Questrade API. Agent-friendly, JSON-first output.",
  "type": "module",
  "bin": {
    "questrade": "./bin/questrade"
  },
  "files": ["bin", "dist"],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "dev": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run typecheck && bun run build"
  },
  "dependencies": {
    "@agrigoryan/questrade-api": "^0.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7"
  },
  "author": "Aram Grigoryan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agrigoryan/questrade-cli"
  },
  "sideEffects": false
}
